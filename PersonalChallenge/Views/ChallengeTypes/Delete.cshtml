@model PersonalChallenge.Models.ChallengeType

@{
    ViewData["Title"] = "Delete";
}




<div id="wrapper">
    <div id="main">
        <div class="inner">
            <header class="major">
                <h2>Are you sure you want to delete this element?</h2>
            </header>
            <section>
               <p>Name: @Model.Name</p>
               <p>Measured in: @Model.MeasuredIn</p>
               <p>Aproved: @Model.Approved</p>
            </section>
            <form asp-action="Delete">
                <input type="hidden" asp-for="Id" />
                <input type="submit" value="Delete" class="button" /> 
                <a asp-action="Index">Back to List</a>
            </form>
        </div>
    </div>
</div>
